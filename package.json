{
  "name": "izg-configuration-console",
  "packageManager": "yarn@3.2.4",
  "private": true,
  "scripts": {
    "api-dev": "yarn workspace @monorepo/api dev",
    "ui-dev": "yarn workspace @monorepo/ui dev",
    "build-api": "yarn workspace @monorepo/api run -T build",
    "build-ui": "yarn workspace @monorepo/ui run build",
    "start-api": "yarn workspace @monorepo/api start",
    "start-ui": "yarn workspace @monorepo/ui start",
    "dev": "concurrently \"yarn api-dev\" \"yarn ui-dev\"",
    "build": "concurrently \"yarn build-api\" \"yarn build-ui\"",
    "start": "concurrently \"yarn start-api\" \"yarn start-ui\"",
    "docker:up": "docker-compose up -d",
    "docker-dev:up": "docker-compose -f ./docker-compose.dev.yml up -d",
    "docker:down": "docker-compose down --remove-orphans",
    "docker-development:up": "yarn docker-dev:up && yarn dev",
    "docker-development:down": "yarn docker:down",
    "prepare": "husky install",
    "format:write": "prettier --write \"packages/**/*.{ts,tsx,js,jsx,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"packages/**/*.{ts,tsx,js,jsx,json}\" --ignore-path .gitignore",
    "lint": "nx run-many --target=lint"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependenciesMeta": {
    "@prisma/client@3.15.2": {
      "unplugged": true
    },
    "prisma@3.15.2": {
      "unplugged": true
    }
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@yarnpkg/pnpify": "^4.0.0-rc.28",
    "concurrently": "^7.5.0",
    "cypress": "^12.9.0",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "moment": "^2.29.4",
    "next-auth": "^4.18.6",
    "prettier": "^2.8.8",
    "uuid": "^9.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "husky": "^8.0.3",
    "typescript": "^5.0.4"
  }
}
